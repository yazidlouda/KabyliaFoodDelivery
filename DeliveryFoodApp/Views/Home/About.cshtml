@{
    ViewBag.Title = "About";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>


@{
    ViewBag.Title = "Home Page";
}
<style>
    #map {
        height: 500px;
    }
</style>
<br />
<br />
<div id="map"></div>

<script>
    var map;
    function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
            center: { lat: 39.913970, lng: -86.110739 },
            zoom: 12
        });
       //BracU
        var markerRestaurant = new google.maps.Marker({
            position: { lat: 39.913970, lng: -86.110739 },
            map: map
        });
        var contentStringRestaurant = '<div id="content">' +
            '<div id="siteNotice">' +
            '</div>' +
            '<h1 id="firstHeading" class="firstHeading">TheCheesecakeFactory</h1>' +
            '<div id="bodyContent">' +
            '<p><b>TheCheesecakeFactory</b>, also referred to as <b>Ayers Rock</b>, is a large ' +
            'sandstone rock formation in the southern part of the ' +
            'Northern Territory, central Australia. It lies 335&#160;km (208&#160;mi) ' +
            'south west of the nearest large town, Alice Springs; 450&#160;km ';
        var infowindowForRestaurant = new google.maps.InfoWindow({
            content: contentStringRestaurant
        });
        markerRestaurant.addListener('mouseover', function () {
            infowindowForRestaurant.open(map, markerRestaurant);
        });
        markerRestaurant.addListener('mouseout', function () {
            infowindowForRestaurant.close();
        });
    }
</script>

<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDJOZd-P3bAJXBAYtfryQT_GTv61uRehMs&callback=initMap" async defer></script>